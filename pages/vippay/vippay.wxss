.titimg {
  height: 269rpx;
  width: 750rpx;
  z-index: -1;
  display: block;
}
page{
  background: #f5f5f5
}
.vipka {
  width: 722rpx;
  height: 377rpx;
  margin: 0 auto;
  background: #363636;
  border-radius: 16rpx;
  margin-top: -230rpx;
  z-index: 2;
  position: relative;
  box-shadow: 0rpx 0rpx 20rpx #000;
  text-align: center;
  color: #FACE9F
}
.vipka>view>image{
  height: 88rpx;
  width: 88rpx;
  margin-top: 50rpx;
}
.vipka>view{
  padding-top: 16rpx;
}
.vipka>view:last-child{
  font-size: 24rpx;
  border: 2rpx #FACE9F solid;
  padding: 10rpx 0;
  border-radius: 28rpx;
  width: 126rpx;
  margin: 0 auto;
  text-align: center;
  margin-top: 20rpx;
}
.list{
  margin-top: 50rpx;
}
.list>view{
  display: flex;
  padding: 20rpx 0;
  font-size: 32rpx;
  background: #fff;
  margin-top: 2rpx;
  position: relative
}
.list>view>view:first-child{
  padding: 0 30rpx;
  color: #333
}
.list>view>view:nth-child(2){
  color: #BF9947
}
.list>view>view:last-child text{
  color:#a4a4a4;
  font-size: 24rpx;
}
.list image{
  height: 24rpx;
  width: 24rpx;
  position: absolute;
  right: 20rpx;
  top: 30rpx;
}
.send{
  padding: 30rpx 0;
  background: #fff
}
.send>view:first-child{
  background: linear-gradient(180deg, rgba(251, 237, 213, 1) 0%, rgba(241, 193, 135, 1) 100%);
  width: 583rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  border-radius: 70rpx;
  margin: 0 auto;
  font-size: 32rpx;
}
.send>view:last-child{
  font-size: 24rpx;
  margin: 0 auto;
  text-align: center;
  color: #4a4a4a;
  margin-top: 16rpx;
}
.send>view:last-child text{
  color: #BF9947
}
.viptime{
  width: 500rpx;
}
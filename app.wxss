/* 获取用户信息弹出层 */

.dialog-mask{
  width:100%;
  height:100%;
  position: fixed;
  left:0;
  top:0;
  background: rgba(0,0,0,0.6);
}
.dialog-container{
  position:relative;
  top:30%;
  left:10%;
  width:80%;
  height:auto; 
  overflow:hidden;
  border-radius:10px;
  background:#fff;
  color:#333;
  opacity:1;
  box-shadow: 0 0 80rpx #333

}
.dialog-header{
  height:80rpx;
  line-height: 80rpx;
  font-size: 32rpx; 
  text-align:center; 
}
.dialog-content{ 
   padding:30rpx;
   font-size: 28rpx;
   color:#999;
}
.dialog-footer{
  display:flex;
}
.dialog-btn{
  width:100%;
  background-color:#fff; 
  padding-left:30rpx;
  padding-right:30rpx;
  border-radius:2px;
  font-size:32rpx;
  line-height:90rpx;
  height:90rpx;
  box-sizing:border-box;
  text-decoration:none;
  text-align:center;
  vertical-align:middle;
  color:#333;
  border:none;
  border-right:1px solid #efefef;
  border-top:1px solid #efefef;
}
.confirm-btn{
  color: #1AAD16
}
.dialog-btn::after{
  border:none;
}